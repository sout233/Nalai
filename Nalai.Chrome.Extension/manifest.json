{
  "manifest_version": 3,
  "name": "Nalai Downloader",
  "version": "1.0",
  "permissions": ["activeTab", "http://localhost:*/", "https://localhost:*/"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}